{% extends 'base.html.twig' %}

{% block title %}AppClass{% endblock %}

{% block body %}
    <h1>AppClass</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ app_class.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ app_class.name }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ app_class.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreationDate</th>
                <td>{{ app_class.creationDate ? app_class.creationDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_class_index') }}">back to list</a>

    <a href="{{ path('app_class_edit', {'id': app_class.id}) }}">edit</a>

    {{ include('app_class/_delete_form.html.twig') }}
{% endblock %}
